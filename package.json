{
  "name": "nodejs-typescript-express-boilerplate",
  "version": "0.01.00",
  "description": "nodejs-typescript-express-boilerplate",
  "main": "src/app.ts",
  "scripts": {
    "start": "npm run serve",
    "test": "cross-env NODE_ENV=test TS_NODE_FILES=true mocha --exit -timeout 600000 -r dotenv/config -r tsconfig-paths/register -r ts-node/register ./test/**/*.test.ts",
    "coverage": "nyc --reporter=lcov npm run test",
    "build": "npm i && npx tsc",
    "build:version": "npx ts-node build.ts",
    "clean": "rimraf dist node_modules package-lock.json",
    "copy:locales": "cp -r ./src/locales ./dist/src/",
    "copy:assets": "cp -r ./src/assets ./dist/src/",
    "depcheck": "npx depcheck",
    "lint": "eslint . --ext .ts",
    "npmcheck": "npx npm-check",
    "package": "pkg . --targets node16-linux-x64 --output idp",
    "presetup": "yarn install",
    "setup": "npm start config && npm start setup.script",
    "serve": "cross-env NODE_ENV=local nodemon --inspect --exec ts-node -r tsconfig-paths/register --files ./src/app.ts"
  },
  "bin": {
    "ics.core": "./dist/src/app.js"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "views/**/*",
      "public/**/*",
      "src/locales/**/*"
    ],
    "targets": [
      "node14-linux-arm64"
    ],
    "outputPath": "dist"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": "https://bitbucket.org/sisoul/idp-api.git",
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express"
  ],
  "homepage": "https://www.icslogistics.co.kr",
  "author": "ohjongin <ji.oh@sisoul.co.kr>",
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.374.0",
    "@aws-sdk/client-s3": "^3.397.0",
    "@aws-sdk/client-sqs": "^3.395.0",
    "@sentry/integrations": "^7.71.0",
    "@sentry/node": "^7.71.0",
    "@sentry/profiling-node": "^1.2.1",
    "@smithy/abort-controller": "^2.0.5",
    "@types/multer": "^1.4.7",
    "accept-language-parser": "^1.5.0",
    "app-root-path": "^3.0.0",
    "assert-fine": "^2.1.0",
    "async": "^3.2.4",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "builder-pattern": "^2.1.0",
